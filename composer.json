{
    "name":"fritzmg/contao-simple-news-urls",
    "description":"Simple Contao extension to allow news URLs without its reader page fragment",
    "keywords":["contao", "news", "urls"],
    "type":"contao-module",
    "homepage":"https://github.com/fritzmg/contao-simple-news-urls",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Fritz Michael Gschwantner",
            "homepage":"http://www.inspiredminds.at",
            "email":"fmg@inspiredminds.at",
            "role":"Developer"
        }
    ],
    "support":{
        "email":"fmg@inspiredminds.at",
        "issues":"https://github.com/fritzmg/contao-simple-news-urls/issues",
        "source":"https://github.com/fritzmg/contao-simple-news-urls",
        "forum":"https://community.contao.org/de"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/fritzmg"
        }
    ],
    "require":{
        "php": ">=7.4",
        "contao/news-bundle": "^4.13 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "InspiredMinds\\ContaoSimpleNewsUrls\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "InspiredMinds\\ContaoSimpleNewsUrls\\ContaoManager\\Plugin"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    }
}
